{
  "type": "module",
  "scripts": {
    "start": "node ./server.js",
    "dev": "concurrently npm:serve npm:webpack:start",
    "serve": "NODE_ENV=development nodemon --ignore ./db.json ./server.js",
    "webpack:start": "NODE_ENV=development webpack serve",
    "webpack:build": "NODE_ENV=production webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/",
    "lint:format": "eslint --fix --ext js,jsx,ts,tsx ./src/"
  },
  "dependencies": {
    "@stanlemon/server-with-auth": "^0.1.1",
    "@stanlemon/webdev": "^0.1.15",
    "@twilio-paste/core": "^11.2.0",
    "@twilio-paste/icons": "^6.2.1",
    "@twilio/conversations": "^2.1.0",
    "dotenv": "^16.0.0",
    "luxon": "^2.3.1",
    "react-chat-ui": "^0.4.0",
    "twilio": "^3.76.0"
  },
  "devDependencies": {
    "@types/luxon": "^2.3.1",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}