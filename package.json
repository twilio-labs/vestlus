{
  "name": "vestlus",
  "version": "0.2.0",
  "description": "A conversational app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/vestlus"
  },
  "type": "module",
  "keywords": [
    "twilio",
    "conversations",
    "chat",
    "vestlus"
  ],
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "node ./server.js",
    "dev": "concurrently npm:serve npm:webpack:start",
    "serve": "NODE_ENV=development nodemon --ignore ./db.json ./server.js",
    "webpack:start": "NODE_ENV=development webpack serve",
    "webpack:build": "NODE_ENV=production webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/",
    "lint:format": "eslint --fix --ext js,jsx,ts,tsx ./src/"
  },
  "dependencies": {
    "@stanlemon/server-with-auth": "^0.1.7",
    "@stanlemon/webdev": "^0.1.56",
    "@twilio-paste/core": "^13.0.0",
    "@twilio-paste/icons": "^8.0.1",
    "@twilio/conversations": "^2.1.0",
    "dotenv": "^16.0.1",
    "luxon": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twilio": "^3.77.3"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@types/luxon": "^2.3.2",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.16"
  }
}